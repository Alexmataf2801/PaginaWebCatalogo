
<!DOCTYPE html>

<html lang="en" class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">

    @*<link href="~/Content/Administracion/bootstrap.min.css" rel="stylesheet" />*@
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Administracion/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/Administracion/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/Administracion/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/datatables.min.css" rel="stylesheet" />

    <title>Administracion</title>

</head>

<body class="hold-transition skin-blue sidebar-mini">

    <form runat="server">

        <div class="wrapper">
            <header class="main-header">
                <!-- Logo -->
                <a href="#" class="logo">
                    <span class="logo-mini">
                        <div class="fa fa-home"></div>
                    </span>
                    <span class="logo-lg">
                        <p style="font-size: x-large"><b>Administración</b></p>
                    </span>

                </a>

                <nav class="navbar navbar-static-top">
                    <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>

                    <div class="navbar-custom-menu">
                        <div style="text-align: center; margin-bottom: 20px">
                            <div class="col-md-12">
                                <div class="row" style="margin: 15px">
                                    <asp:Label runat="server" CssClass="EsconderNombre" ID="lblNombre" Style="color: #FFFFFF; font-weight: bold; padding: 20px; margin-top: 20px">Bienvenido(a): @ViewBag.Usuario</asp:Label>
                                </div>
                            </div>
                        </div>
                        <ul class="nav navbar-nav">
                            <li>
                                @*<%--NO BORRAR--%>*@
                                <asp:LinkButton runat="server" ID="BtnCerrarSesion" OnClick="BtnCerrarSesion_Click" CssClass="fa fa-sign-out "></asp:LinkButton>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <aside class="main-sidebar">
                <section class="sidebar">
                    <div class="user-panel">
                        <div class="imagenLogo">
                            <a href="../../../INSServicios.SICOA/Default">
                                @*<img src='<%=Page.ResolveUrl("~/Imagenes/Logos/Logo.png") %>'>*@
                            </a>
                        </div>

                        <div class="pull-left info">
                            @*<img src="Content/img/Logo.png">*@
                        </div>
                    </div>

                    <div runat="server" id="Menu">
                        @Html.Raw(@ViewBag.Menu)
                    </div>


                </section>
                <!-- /.sidebar -->
            </aside>

            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <div class="" style="margin-left: 7px;">
                    @RenderBody()
                </div>
                <!-- /.content -->
            </div>
            <!-- /.content-wrapper -->
            <footer class="main-footer">
                <div style="text-align: center">
                    @*<strong>Copyright &copy;  @DateTime.Now.Year <a target="_blank" href="http://www.insservicios.com/">INS Servicios S.A</a>.</strong>*@
                </div>
            </footer>
        </div>


        <script src="~/Js/Administracion/jquery.min.js"></script>
        <script src="~/Js/Administracion/bootstrap.min.js"></script>
        <script src="~/Js/Administracion/adminlte.min.js"></script>
        <script src="~/Scripts/Mantenimientos/Mantenimientos.js"></script>
        <script src="~/Js/datatables.min.js"></script>


    </form>


</body>
</html>